{
  "extends": "airbnb",
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "page": true,
    "browser": true,
    "context": true,
    "document": true
  },
  "rules": {
    "eol-last": 0,
    "import/prefer-default-export": 0,
    "no-await-in-loop": 0,
    "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
    "no-trailing-spaces": 0,
    "max-len": 0,
    "no-unused-vars": 1,
    "no-multiple-empty-lines": 0,
    "padded-blocks": 0,
    "func-names": 0,
    "no-console": 0,
    "arrow-parens": 0,
    "semi": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "no-empty": 0,
    "prefer-const": 0,
    "no-prototype-builtins": 0,
    "no-shadow": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "no-bitwise": 0,
    "import/order": 0
  }
}
